{
	"lock": false,
	"name": "sveltekit-grammar-check",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin=prettier-plugin-svelte . --check . && eslint .",
		"lint:css": "stylelint \"src/**/*.{css,svelte}\"",
		"format": "prettier --plugin=prettier-plugin-svelte . --write ."
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^19.2.2",
		"@iconify/svelte": "^4.0.2",
		"@sveltejs/adapter-netlify": "^4.2.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"commitlint": "^19.3.0",
		"eslint": "^9.2.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.38.0",
		"husky": "^9.0.11",
		"postcss": "^8.4.38",
		"postcss-csso": "^6.0.1",
		"postcss-custom-media": "^10.0.6",
		"postcss-html": "^1.6.0",
		"postcss-preset-env": "^9.5.11",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"stylelint": "^16.5.0",
		"stylelint-config-recommended": "^14.0.0",
		"stylelint-config-standard": "^36.0.0",
		"svelte": "^4.2.16",
		"svelte-check": "^3.7.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/sveltekit-grammar-check"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/sveltekit-grammar-check/issues"
	},
	"type": "module"
}
